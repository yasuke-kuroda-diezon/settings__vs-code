  "chat.agent.maxRequests": 200,
  "chat.agentSessionsViewLocation": "view",
  "chat.emptyState.history.enabled": true,
  "chat.mcp.autostart": "newAndOutdated",
  "chat.promptFiles": true,
  "chat.todoListTool.enabled": true,
  "chat.tools.terminal.autoApprove": {
    "cd": true,
    "echo": true,
    "ls": true,
    "pwd": true,
    "cat": true,
    "head": true,
    "tail": true,
    "findstr": true,
    "which": true,
    "basename": true,
    "dirname": true,
    "du": true,
    "df": true,
    "git branch": true,
    "git commit": true,
    "git diff": true,
    "git log": true,
    "git remote": true,
    "git show": true,
    "git status": true,
    "/git push -u origin feature\\/.*$/": true,
    "/git push -u origin fix\\/.*$/": true,
    "/git push -u origin hotfix\\/.*$/": true,
    "column": true,
    "/^column\\b.*-c\\s+[0-9]{4,}/": false,
    "date": true,
    "/^date\\b.*(-s|--set)\\b/": false,
    "find": true,
    "/^find\\b.*-(delete|exec|execdir|fprint|fprintf|fls|ok|okdir)\\b/": false,
    "grep": true,
    "/^grep\\b.*-(f|P)\\b/": false,
    "sort": true,
    "/^sort\\b.*-(o|S)\\b/": false,
    "tree": true,
    "/^tree\\b.*-o\\b/": false,
    "/\\(.+\\)/": {
      "approve": false,
      "matchCommandLine": true
    },
    "/\\{.+\\}/": {
      "approve": false,
      "matchCommandLine": true
    },
    "/`.+`/": {
      "approve": false,
      "matchCommandLine": true
    },
    "rm": false,
    "rmdir": false,
    "del": false,
    "Remove-Item": false,
    "ri": false,
    "rd": false,
    "erase": false,
    "dd": false,
    "kill": false,
    "ps": false,
    "top": false,
    "Stop-Process": false,
    "spps": false,
    "taskkill": false,
    "taskkill.exe": false,
    "curl": false,
    "wget": false,
    "Invoke-RestMethod": false,
    "Invoke-WebRequest": false,
    "irm": false,
    "iwr": false,
    "chmod": false,
    "chown": false,
    "Set-ItemProperty": false,
    "sp": false,
    "Set-Acl": false,
    "jq": false,
    "xargs": false,
    "eval": false,
    "Invoke-Expression": false,
    "iex": false
  },
  "debug.showInStatusBar": "never",
  "diffEditor.experimental.useTrueInlineView": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorWidth": 1,
  "editor.dragAndDrop": false,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.minimap.autohide": "scroll",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "always",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.showFoldingControls": "always",
  "editor.suggest.showStatusBar": true,
  "editor.tabSize": 2,
  "errorLens.statusBarIconsEnabled": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.cache/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.history/**": true,
