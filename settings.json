    "**/node_modules/**": true,
    "**/out/**": true,
    "**/tmp/**": true
  },
  "git.autorefresh": true,
  "git.blame.statusBarItem.enabled": false,
  "git.decorations.enabled": false,
  "git.enableStatusBarSync": false,
  "github.copilot.chat.agent.thinkingTool": true,
